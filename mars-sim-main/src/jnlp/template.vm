<?xml version="1.0" encoding="utf-8"?>
<jnlp
    spec="1.0+"
    codebase="http://www.landrus.de/mars-sim/"
    href="$outputFile">
  <information>
    <title>Mars Simulation Project</title>
    <homepage href="http://mars-sim.sourceforge.net/"/>
    <description kind="one-line">Mars Simulation Project</description>
    <description kind="short">Mars Simulation Project</description>
    <description kind="tooltip">Mars Simulation Project</description>
    <vendor>Mars Simulation Project</vendor>
