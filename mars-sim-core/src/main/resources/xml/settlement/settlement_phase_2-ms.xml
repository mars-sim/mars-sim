<?xml version = "1.0" encoding = "UTF-8" standalone = "yes" ?>
<template xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="settlement.xsd"
    name="Phase 2-MS" default-population="16" description="Phase 2 base for Mars Society"
    number-of-robots="5" sponsor="MS">
    
    <building id="LH0" type="Lander Hab" xloc="0.0" yloc="0.0"
        facing="180.0">
        <connection-list>
            <connection id="C00" hatch-facing="east" />
        </connection-list>
    </building>

   <building id="C00" type="Corridor 3x9" xloc="-6.0" yloc="0.0"
        facing="0.0">
        <connection-list>
            <connection id="LH0" hatch-facing="west" />
            <connection id="EVA" hatch-facing="south" />
            <connection id="C1" hatch-facing="north" />
        </connection-list>
    </building>
    
    <building id="EVA" type="EVA Airlock" xloc="-6.0" yloc="-7.5"
        facing="270.0">
        <connection-list>
            <connection id="C00" hatch-facing="north" />
        </connection-list>
    </building>

    <building id="C1" type="Corridor 3x3" xloc="-6.0" yloc="6.0"
        facing="0.0">
        <connection-list>
            <connection id="C00" hatch-facing="south" />
            <connection id="C2" hatch-facing="east" />
			<connection id="C01" hatch-facing="north" />
        </connection-list>
    </building>
    
    <building id="C2" type="Corridor 3x9" xloc="-12.0" yloc="6.0"
        facing="90.0">
        <connection-list>
            <connection id="C1" hatch-facing="west" />
            <connection id="C3" hatch-facing="east" />
            <connection id="L0" hatch-facing="south" />
 			<connection id="IG0" hatch-facing="north" />            
        </connection-list>
    </building>
    
    <building id="L0" type="Laboratory" xloc="-12.0" yloc="1.0"
        facing="270.0">
        <connection-list>
            <connection id="C2" hatch-facing="north" />
        </connection-list>
    </building>

    <building id="IG0" type="Inflatable Greenhouse" xloc="-12.0" yloc="10.5"
        facing="270.0">
        <connection-list>
            <connection id="C2" hatch-facing="south" />
         	<connection id="IG1" hatch-facing="east" />
        </connection-list>
    </building>

    <building id="C3" type="Corridor 3x9" xloc="-21.0" yloc="6.0"
        facing="90.0">
        <connection-list>
            <connection id="C2" hatch-facing="west" />
            <connection id="W0" hatch-facing="south" />
            <connection id="IG1" hatch-facing="north" />
        </connection-list>
    </building>
    
    <building id="W0" type="Workshop" xloc="-21.0" yloc="1.0" 
    	facing="90.0">
        <connection-list>
            <connection id="C3" hatch-facing="north" />
        </connection-list>
    </building>
    
    <building id="IG1" type="Inflatable Greenhouse" xloc="-21.0" yloc="10.5"
        facing="270.0">
        <connection-list>
            <connection id="C3" hatch-facing="south" />
         	<connection id="IG0" hatch-facing="west" />
        </connection-list>
    </building>


    <building id="LH1" type="Lander Hab" xloc="0.0" yloc="12.0"
        facing="180.0">
        <connection-list>
            <connection id="C01" hatch-facing="east" />
        </connection-list>
    </building>


    <building id="C01" type="Corridor 3x9" xloc="-6.0" yloc="12.0"
        facing="0.0">
        <connection-list>
            <connection id="C1" hatch-facing="south" />
           <connection id="LH1" hatch-facing="west" />
        </connection-list>
    </building>


    <!-- Building Packages -->
    <building-package name="Storage Set 1"/>
   	<building-package name="ERV Set 1"/>
   	<building-package name="Air Set 1 - 1x2"/>
    <building-package name="Sabatier Set 1 - 1x3"/> 
	<building-package name="Syngas Set 1 - 1x1"/>
	<building-package name="Solar Set 1 - 1x3"/>
	<building-package name="Solar Thermal Set 1 - 1x3"/>
	<building-package name="Kilo Set 1 - 1x2"/>
	<building-package name="MD1 Set 1 - 1x1"/>
 	<building-package name="Brine Water Well Set 1 - 1x1"/>	 

    <vehicle type="Explorer Rover" number="2" />
    <vehicle type="Light Utility Vehicle" number="1" />
    <vehicle type="Delivery Drone" number="1" />

    <equipment type="EVA Suit" number="16" />

    <!-- Containers-->
	<equipment type="barrel" number="40" />
	<equipment type="bag" number="40" />
	<equipment type="gas canister" number="40" />		
	<equipment type="large bag" number="40" />
	<equipment type="specimen box" number="40" />
    <equipment type="thermal bottle" number="12" />	
 	<equipment type="wheelbarrow" number="12" />
 	
	<!-- Life Support Resources -->
	<resource type="food" amount="1440" />
	<resource type="water" amount="4800" />
	<resource type="oxygen" amount="2880" />
 
    <part-package name="standard package" number="2" />
 
    <resupply>
       	<resupply-mission name="BiMonthly Delivery" manifest-name="Standard Resupply 1">
                    <schedule firstSol="3" frequency="60" />
        </resupply-mission>
        <resupply-mission name="Annual Delivery" manifest-name="Resupply for Phase 2">
                    <schedule firstSol="668" />
        </resupply-mission>
    </resupply>
</template>